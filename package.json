{
  "name": "hybricmark",
  "private": false,
  "version": "0.1.0",
  "description": "A headless, Typora-like Markdown editor for React, built on Tiptap.",
  "license": "MIT",
  "keywords": [
    "tiptap",
    "markdown",
    "editor",
    "react",
    "wysiwyg",
    "headless",
    "typora"
  ],
  "homepage": "https://your-vercel-link-placeholder.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/hybricmark.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/hybricmark/issues"
  },
  "type": "module",
  "main": "./dist/hybricmark.umd.js",
  "module": "./dist/hybricmark.es.js",
  "types": "./dist/hybricmark.es.d.ts",
  "files": [
    "dist/hybricmark.css",
    "dist/hybricmark.es.d.ts",
    "dist/hybricmark.es.js",
    "dist/hybricmark.umd.js",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/hybricmark.es.d.ts",
      "import": "./dist/hybricmark.es.js",
      "require": "./dist/hybricmark.umd.js"
    },
    "./style.css": "./dist/hybricmark.css"
  },
  "sideEffects": [
    "./dist/hybricmark.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.16",
    "@tiptap/extension-highlight": "^3.19.0",
    "@tiptap/extension-image": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-list": "^3.19.0",
    "@tiptap/extension-mathematics": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-subscript": "^3.19.0",
    "@tiptap/extension-superscript": "^3.19.0",
    "@tiptap/extension-table": "^3.19.0",
    "@tiptap/extension-table-cell": "^3.19.0",
    "@tiptap/extension-table-header": "^3.19.0",
    "@tiptap/extension-table-row": "^3.19.0",
    "@tiptap/extension-typography": "^3.19.0",
    "@tiptap/extension-underline": "^3.19.0",
    "@tiptap/extension-unique-id": "^3.19.0",
    "@tiptap/markdown": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "clsx": "^2.1.1",
    "katex": "^0.16.28",
    "lucide-react": "^0.563.0",
    "markdown-it": "^14.1.1",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^8.0.0-beta.13",
    "vite-plugin-dts": "^4.5.4"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  }
}
